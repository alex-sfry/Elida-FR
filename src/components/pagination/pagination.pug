nav(aria-label='Search result pagination')
    ul.pagination.justify-content-center
        li.page-item.disabled
            span.page-link Previous
        li.page-item.active(aria-current='page')
            a.page-link(href='#') 1
        li.page-item
            span.page-link 2
        li.page-item
            a.page-link(href='#') 3
        li.page-item
            a.page-link(href='#') Next